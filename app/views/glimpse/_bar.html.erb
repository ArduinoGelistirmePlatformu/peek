<% if glimpse_enabled? %>
<div id="glimpse" class="<%= Glimpse.env %><%= ' disabled' if cookies[:glimpse] == 'false' %>">
  <div class="wrapper">
    <% Glimpse.views.each do |view| %>
      <div id="<%= view.dom_id %>" class="view">
        <%= render view.partial_path, :view => view %>
      </div>
    <% end %>
  </div>
</div>
<% end %>
